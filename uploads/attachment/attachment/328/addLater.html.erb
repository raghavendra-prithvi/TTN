<div class="col-xs-12"><%if !@internals.blank?%>Internal Project Assignment<div style="float:left;width:50%;margin-top:60px"><div style="width:40%;float:left;">    <b>Select User(s)</b><br/>    <%= collection_select(:users, :id, @r_users, :id, :name, options ={:prompt => "-Select Users"}, :class =>"project", :multiple => true, :style => "height:100px;width:100%;") %>  </div>  <div style="width:60%;float:left;">    <b>Select Project(s)</b></br>	<%= collection_select(:project, :id, @internals, :id, :title, options ={:prompt => "-Select a project"}, :class =>"project", :multiple => true, :style => "height:100px;width:66.66%;float:left;") %>    </div><input type="button" class="btn btn-medium btn-primary" value="Assign Internal Project" style="margin-top:20px;margin-left:200px" id="assign_internal_project"></input></div><div class="pull-left" style="width:40%;margin-left:5px">  <div id="projects_assigned">  <table class="table-bordered table" style="width:100%">    <caption><h3>Project User Hierarchy</h3></caption>  <thead>    <tr>      <th>        Project      </th>      <th>        Team      </th>    </tr>  </thead><% @project_users.each do |puk,puv| %>  <tbody>    <tr>      <td style="color:green">        <% @p = Project.find(puk).name %>                <%= @p %>        <input type="checkbox" name="id" class="pull-right" onclick='$(this).parent().parent().find("input:checkbox").prop("checked", this.checked)' title="Select All" ></input>      </td>      <% puv.each do |v| %>        	<%empId = v.user_id%>      	<%if !Reporter.where(:reporter_id => @user.id, :reported_id => empId).blank?%>        <td>          <%=User.find(v.user_id).name %>          <input type="checkbox" name="id" class="pull-right #{@p}" value ="this.id"  id=<%=v.user_id.to_s + "_"+puk.to_s %> ></input>          <!-- <button onclick="removeUserProject(this.id)" id=<%=v.user_id.to_s + "_"+puk.to_s%> ><i class="icon-trash"></i></button> -->        </td>            <%end%>      <% end %>    </tr>  </tbody><% end %></table></div><button onclick="removeSelectedUserProject()" class="btn btn-primary" >Remove the User(s)</button><%end%></div></div><div class="col-xs-12">    <%if !@work_orders.blank?%>Work Order Assignment<div style="float:left;width:50%;margin-top:60px"><div style="width:40%;float:left;">    <b>Select User(s)</b><br/>    <%= collection_select(:users, :id, @r_users, :id, :name, options ={:prompt => "-Select Users"}, :class =>"project", :multiple => true, :style => "height:100px;width:100%;") %>  </div>  <div style="width:60%;float:left;">    <b>Select Project(s)</b></br>	<%= collection_select(:project, :id, @work_orders, :id, :project_name, options ={:prompt => "-Select a project"}, :class =>"project", :multiple => true, :style => "height:100px;width:66.66%;float:left;") %>  <%end%>  </div><input type="button" class="btn btn-medium btn-primary" value="Assign Work Order" style="margin-top:20px;margin-left:200px" id="assign_wo"></input></div><div class="pull-left" style="width:40%;margin-left:5px">  <div id="projects_assigned">  <table class="table-bordered table" style="width:100%">    <caption><h3>Project User Hierarchy</h3></caption>  <thead>    <tr>      <th>        Project      </th>      <th>        Team      </th>    </tr>  </thead><% @project_users.each do |puk,puv| %>  <tbody>    <tr>      <td style="color:green">        <% @p = Project.find(puk).name %>                <%= @p %>        <input type="checkbox" name="id" class="pull-right" onclick='$(this).parent().parent().find("input:checkbox").prop("checked", this.checked)' title="Select All" ></input>      </td>      <% puv.each do |v| %>        	<%empId = v.user_id%>      	<%if !Reporter.where(:reporter_id => @user.id, :reported_id => empId).blank?%>        <td>          <%=User.find(v.user_id).name %>          <input type="checkbox" name="id" class="pull-right #{@p}" value ="this.id"  id=<%=v.user_id.to_s + "_"+puk.to_s %> ></input>          <!-- <button onclick="removeUserProject(this.id)" id=<%=v.user_id.to_s + "_"+puk.to_s%> ><i class="icon-trash"></i></button> -->        </td>            <%end%>      <% end %>    </tr>  </tbody><% end %></table></div><button onclick="removeSelectedUserProject()" class="btn btn-primary" >Remove the User(s)</button><%end%></div>