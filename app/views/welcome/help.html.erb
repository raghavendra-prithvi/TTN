<script type="text/javascript">
      window.onload = function (){
        var success = new PDFObject({ url: "/assets/EffortTrackerManualAdmin.pdf" }).embed();
      };
    </script>




<%= javascript_tag do %>
  window.onload = function (){
      <% if @current_user.admin == true%>
                  var success = new PDFObject({ url: "/assets/EffortTrackerManualAdmin.pdf" }).embed();                    
      <% elsif @current_user.time_sheet_manager == true %>
              var success = new PDFObject({ url: "/assets/EffortTrackerManualtsm.pdf" }).embed();         
      <% elsif @current_user.time_sheet_collaborator == true %>
              var success = new PDFObject({ url: "/assets/EffortTrackerManualtsc.pdf" }).embed();                
      <% elsif @current_user.work_order_admin == true %>
                  var success = new PDFObject({ url: "/assets/EffortTrackerManualWoAdmin.pdf" }).embed();
      <% elsif @current_user.manager == true %>
                  var success = new PDFObject({ url: "/assets/EffortTrackerManualManager.pdf" }).embed();

      <% elsif @current_user.role =="Classified" || @current_user.role =="Unclassified"%>
                    var success = new PDFObject({ url: "/assets/EffortTrackerManualEmployee.pdf" }).embed();

      <% elsif @current_user.client == true %>
                      var success = new PDFObject({ url: "/assets/EffortTrackerManualWoAdmin.pdf" }).embed();

      <% else @current_user.role =="GA" %>
                        var success = new PDFObject({ url: "/assets/EffortTrackerManualGa.pdf" }).embed();
                        
      <%end%> 
      
  }
<% end %>

